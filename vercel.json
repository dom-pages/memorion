{
  "name": "memorion",
  "env": {
    "NEXT_PUBLIC_DOMAIN": "healthprosperity.site",
    "NEXT_PUBLIC_SITE_URL": "https://healthprosperity.site"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        }
      ]
    }
  ]
} 